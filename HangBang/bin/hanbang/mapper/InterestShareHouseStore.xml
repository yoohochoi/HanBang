<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="hanbang.store.mapper.InterestShareHouse">

	<insert id="create" parameterType="int">

		INSERT INTO
		INTERESTSHAREHOUSE_TB(shareHouseId, memberId)
		VALUES(#{sharehouseId},
		#{memberId})
	</insert>

	<select id="retriveAll" parameterType="String" resultType="ShareHouse">
		SELECT shareHouseId , memberId
		FROM INTERESTSHAREHOUSE_TB
		WHERE MEMBERID
		= #{memberId}
	</select>

	<delete id="delete" parameterType="int">
		DELETE FROM
		INRERESTSHAREHOUSE_TB
		WHERE SHAREHOUSEID= #{shareHouseId}
	</delete>

	<delete id="deleteByMemberId" parameterType="String">
		DELETE FROM
		INRERESTSHAREHOUSE_TB
		WHERE  MEMBERID
		= #{memberId}
	</delete>

	<delete id="deleteInterestShareHouse" parameterType="int">
		DELETE FROM
		INRERESTSHAREHOUSE_TB
		WHERE SHAREHOUSEID= #{shareHouseId}
	</delete>
</mapper>
