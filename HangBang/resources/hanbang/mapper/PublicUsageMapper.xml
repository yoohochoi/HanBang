<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="hanbang.store.mapper.PublicUseageMapper">

	<insert id="create" parameterType="PublicUsage">
		<selectKey keyProperty="publicUsageId" resultType="Integer"
			order="BEFORE">
			SELECT publicUsageId_seq.nextval FROM dual
		</selectKey>
		INSERT INTO PUBLICUSAGE_TB(HOUSEID, PUBLICUSAGEID, PUBLICUSAGE)
		VALUES(#{houseId}, #{publicUsageId}, #{publicUsage})
	</insert>

	<select id="retriveAll" resultType="PublicUsage" parameterType="int">
		SELECT PUBLICUSAGEID, HOUSEID, PUBLICUSAGE
		FROM PUBLICUSAGE_TB
		WHERE HOUSEID = #{houseId}
	</select>

	<delete id="delete" parameterType="int">
		DELETE FROM PUBLICUSAGE_TB
		WHERE PUBLICUSAGEID = #{publicUsageId}
	</delete>

	<delete id="deleteByHouseId" parameterType="int">
		DELETE FROM
		PUBLICUSAGE_TB
		WHERE HOUSEID = #{houseId}
	</delete>
</mapper>


	