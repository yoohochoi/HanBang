<mapper namespace="hanbang.store.mappper.ProvidedGoodMapper">

	<insert id="create">
		<selectKey keyProperty="providedGoodId" resultType="int">
			SELECT
			PROVIDEDGOODID_seq.nextval FROM dual
		</selectKey>
		INSERT INTO
		providedGoods_tb(providedGoodId, extraInfoId, providedGood)
		VALUES(#{providedGoodId}, #{extraInfoId}, #{providedGood})
	</insert>

